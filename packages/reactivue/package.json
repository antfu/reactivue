  "devDependencies": {
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "cac": "^6.6.1",
    "preact": "^10.5.9",
    "react": "^17.0.1",
    "rollup": "^2.33.0",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-typescript2": "^0.29.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "preact": "^10.5.9",
    "react": ">=16"
