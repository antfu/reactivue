{
  "name": "reactivue",
  "description": "Using Vue Composition API in React components.",
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "version": "0.2.7",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./": "./",
    "./preact": {
      "import": "./preact/index.esm.js",
      "require": "./preact/index.js"
    }
  },
  "files": [
    "dist",
    "preact"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/reactivue.git"
  },
  "keywords": [
    "vue",
    "react",
    "vue-composition-api",
    "vue-in-react",
    "reactivity"
  ],
  "bugs": {
    "url": "https://github.com/antfu/reactivue/issues"
  },
  "homepage": "https://github.com/antfu/reactivue#readme",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "babel-jest": "^26.6.3",
    "cac": "^6.7.1",
    "jest": "^26.6.3",
    "preact": "^10.5.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.36.1",
    "rollup-plugin-dts": "^1.4.14",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "preact": "^10.5.9",
    "react": ">=16"
  },
  "dependencies": {
    "@vue/reactivity": "^3.0.2",
    "@vue/runtime-core": "^3.0.5"
  }
}
