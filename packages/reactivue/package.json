{
  "name": "reactivue",
  "version": "0.4.4",
  "description": "Using Vue Composition API in React components.",
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "browser": "dist/index.module.js",
  "homepage": "https://github.com/antfu/reactivue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/reactivue.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/reactivue/issues"
  },
  "keywords": [
    "vue",
    "react",
    "vue-composition-api",
    "vue-in-react",
    "reactivity"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "production": "./dist/index.module.js",
        "development": "./dist/index.module.dev.js"
      },
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./preact": {
      "types": "./dist/index.d.ts",
      "browser": {
        "production": "./preact/index.module.js",
        "development": "./preact/index.module.dev.js"
      },
      "require": "./preact/index.js",
      "import": "./preact/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "preact"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest"
  },
  "peerDependencies": {
    "preact": "^10.5.9",
    "react": ">=16"
  },
  "dependencies": {
    "@vue/reactivity": "^3.3.4",
    "@vue/runtime-core": "^3.3.4",
    "@vue/shared": "^3.3.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^18.2.4",
    "babel-jest": "^29.5.0",
    "cac": "^6.7.14",
    "jest": "^29.5.0",
    "preact": "^10.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  }
}
