{
  "name": "reactivue",
  "version": "1.0.0-beta.7",
  "description": "Use Vue 3 Composition API in React and Solid components",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/react.cjs",
  "module": "./dist/react.js",
  "types": "./dist/react.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react.cjs",
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./solid": {
      "require": "./dist/solid.cjs",
      "import": "./dist/solid.js",
      "types": "./dist/solid.d.ts"
    },
    "./babel": {
      "require": "./dist/babel.cjs",
      "import": "./dist/babel.js",
      "types": "./dist/babel.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "solid": [
        "dist/solid.d.ts"
      ],
      "babel": [
        "dist/babel.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish --tag beta",
    "test": "vitest",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "tsup": {
    "entry": [
      "src/react.tsx",
      "src/babel.ts",
      "src/solid.tsx"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "external": [
      "react",
      "react-dom",
      "solid-js",
      "@vue/runtime-core"
    ],
    "splitting": false,
    "sourcemap": "inline",
    "dts": true,
    "clean": true
  },
  "files": [
    "dist",
    "bin",
    "scripts"
  ],
  "bin": {
    "reactivue-fix": "bin/reactivue-fix.js"
  },
  "homepage": "https://github.com/antfu/reactivue#readme",
  "bugs": {
    "url": "https://github.com/antfu/reactivue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/reactivue.git"
  },
  "keywords": [
    "vue",
    "react",
    "solid",
    "vue-composition-api",
    "vue-in-react",
    "reactivity"
  ],
  "peerDependencies": {
    "@vue/runtime-core": "^3.2.31",
    "react": "^17.0.2||^18.0.0",
    "solid-js": "^1.3.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.14.2",
    "@antfu/ni": "^0.12.0",
    "@babel/types": "^7.17.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@unocss/transformer-directives": "^0.26.2",
    "@vitejs/plugin-react": "^1.2.0",
    "@vue/runtime-core": "^3.2.31",
    "bumpp": "^7.1.1",
    "eslint": "^8.9.0",
    "esno": "^0.13.0",
    "jsdom": "^19.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "solid-js": "^1.3.8",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "unocss": "^0.26.2",
    "vite": "^2.8.4",
    "vitest": "^0.0.129"
  }
}
